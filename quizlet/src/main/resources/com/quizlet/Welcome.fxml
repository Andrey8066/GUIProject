<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<StackPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
    fx:controller="com.quizlet.Welcome">
    <HBox alignment="CENTER" spacing="10">
        <children>
            <Button text="Создать квиз" onAction="#newQuiz" prefWidth="200" prefHeight="80"></Button>
            <Button text="Посмотреть мои квизы" onAction="#openQuizes" prefWidth="200"
                prefHeight="80"></Button>
            <Button text="Статистика" onAction="#Statistic" prefWidth="200" prefHeight="80"></Button>
        </children>
        <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" />
        </padding>
    </HBox>
</StackPane>